{
  "name": "backend",
  "version": "1.0.0",
  "description": "EMSys (Engine Monitoring System) Backend API",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "npm run prisma:generate && node --watch src/server.js",
    "build": "npm run prisma:generate",
    "start": "npm run prisma:push && node src/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push --accept-data-loss --skip-generate",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "prisma": "^5.20.0"
  }
}
